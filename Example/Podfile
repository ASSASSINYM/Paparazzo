source 'ssh://git@stash.msk.avito.ru:7999/ma/avito-pod-specs.git'
source 'https://github.com/CocoaPods/Specs.git'

inhibit_all_warnings!
use_frameworks!

target 'PaparazzoExample' do
  pod 'Paparazzo', :path => '../'
  pod 'Marshroute', '0.3.5'
  #  Because local, if not local enough "pod 'ImageSource"
  pod 'ImageSource/Core', :path => '../'
  pod 'ImageSource/PHAsset', :path => '../'
  pod 'ImageSource/Local', :path => '../'
  pod 'ImageSource/Remote', :path => '../'
  pod 'ImageSource/AlamofireImage', :path => '../'
end

target 'PaparazzoExample_NoMarshroute' do
  pod 'Paparazzo/Core', :path => '../'
  pod 'Paparazzo/SDWebImage', :path => '../'
  pod 'ImageSource/Core', :path => '../'
  pod 'ImageSource/PHAsset', :path => '../'
  pod 'ImageSource/Local', :path => '../'
  pod 'ImageSource/Remote', :path => '../'
  pod 'ImageSource/SDWebImage', :path => '../'
end

